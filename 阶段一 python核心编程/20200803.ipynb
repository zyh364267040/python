   "source": [
    "# PythonDraw.py(蟒蛇绘制)\n",
    "\n",
    "import turtle as t\n",
    "\n",
    "t.setup(650, 350)\n",
    "t.penup()\n",
    "t.fd(-250)\n",
    "t.pendown()\n",
    "t.pensize(25)\n",
    "t.pencolor('red')\n",
    "t.seth(-40)\n",
    "for i in range(4):\n",
    "    t.circle(40, 80)\n",
    "    t.circle(-40, 80)\n",
    "t.circle(40, 80/2)\n",
    "t.fd(40)\n",
    "t.circle(16, 180)\n",
    "t.fd(40 * 2/3)\n",
    "t.done()"
   ]
  }
